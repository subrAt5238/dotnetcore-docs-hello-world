pipeline {
    agent {label "JDK-11-NUGET"}
		stages {
			stage('clone'){
				steps {
					git url:'https://github.com/subrAt5238/dotnetcore-docs-hello-world.git', 
					branch :'subrat'
					}
						}
			stage('build'){
					steps {
						sh 'dotnet build dotnetcoresample.csproj'
							}
								}
			stage ('package'){
					steps {
						sh 'dotnet publish dotnetcoresample.csproj'
						}					
				}
		}
}
